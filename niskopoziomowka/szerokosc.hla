program szerokosc;
#include ("stdlib.hhf");

var
	zmienna: int16;
	licznik_kol: int16;

begin szerokosc;
	mov(40,zmienna);
	mov(0,licznik_kol);
	
	while(zmienna>0) do
	if(licznik_kol==5) then
	stdout.newln();
	mov(0,licznik_kol);
	endif;
	stdout.put(zmienna:5);
	sub(1,zmienna);
	add(1,licznik_kol);

	
endwhile;

end szerokosc;